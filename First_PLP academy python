FirstNum = float(input("Enter the first number"))
SecondNum = float(input("Enter second number "))
Value = FirstNum + SecondNum
Value2 = FirstNum - SecondNum
Value3 = FirstNum / SecondNum
Value4 = FirstNum % SecondNum
print("FirstNum + SecondNum =", Value)
print("FirstNum - SecondNum =", Value2)
print("FirstNum / SecondNum =", Value3)
print("FirstNum % SecondNum =", Value4)
